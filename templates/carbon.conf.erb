#
# generated by puppet
# Module:   <%= scope.to_hash['module_name'] %>
# Template: <%= file %>
#
# For documentation see:
# http://graphite.readthedocs.org/en/latest/config-carbon.html#carbon-conf
# https://github.com/graphite-project/carbon/blob/master/conf/carbon.conf.example
#
<%
    #carbon = scope.lookupvar('graphite_omnibus::config::carbon')
    carbon = @carbon
    if carbon
        carbon.sort.each do |section, params| %>
[<%= section %>]
        <%- params.sort.each do |key, value| -%>
            <%- if value.kind_of?(Array) -%>
<%= key %> = <%= value.join(', ') %>
            <%- else -%>
<%= key %> = <%= value %>
            <%- end -%>
        <%- end -%>
    <%- end -%>
<%- end %>
