<%
    #rules = scope.lookupvar('graphite_omnibus::config::rewrite_rules')
    rules = @rewrite_rules
    if rules
        rules.sort.each do |section, params| %>
[<%= section %>]
        <%- params.sort.each do |key, value| -%>
<%= key %> = <%= value %>
        <%- end -%>
    <%- end -%>
<%- end -%>
